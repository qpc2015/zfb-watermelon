@import "../common/index.acss";
.van-tabbar {
    display: -webkit-flex;
    display: flex;
    width: 100%;
    height: 50px;
    background-color: #fff;
    justify-content: space-around;
}

.van-tabbar--fixed {
    position: fixed;
    bottom: 0;
    left: 0;
}

.van-tabbar--safe {
    padding-bottom: 34px;
}
